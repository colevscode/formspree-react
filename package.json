{
  "name": "@statickit/react",
  "version": "1.0.0-beta.5",
  "description": "The React component library for StaticKit",
  "homepage": "https://statickit.com",
  "author": "Derrick Reimer",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/unstacked/statickit-react.git"
  },
  "main": "dist/statickit-react.umd.js",
  "module": "dist/statickit-react.esm.js",
  "scripts": {
    "build": "rollup -c",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@statickit/core": "^1.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-react": "^7.0.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "react": ">= 16.8.0",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add dist/"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "npx prettier --write",
      "git add"
