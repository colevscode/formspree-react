{
  "name": "@statickit/react",
  "version": "1.1.2",
  "description": "The React component library for StaticKit",
  "homepage": "https://statickit.com/docs/react",
  "bugs": {
    "url": "https://github.com/unstacked/statickit-react/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unstacked/statickit-react.git"
  },
  "license": "MIT",
  "author": "Derrick Reimer",
  "files": [
    "dist/statickit-react.esm.js",
    "dist/statickit-react.cjs.js",
    "dist/statickit-react.iife.js"
  ],
  "main": "dist/statickit-react.cjs.js",
  "module": "dist/statickit-react.esm.js",
  "scripts": {
    "build": "rollup -c",
    "prepare": "npm run build",
    "release": "np",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "npx prettier --write",
      "git add"
    ],
    "package.json": [
      "npx sort-package-json",
      "git add"
    ]
  },
  "dependencies": {
    "@statickit/core": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.3",
    "babel-jest": "^24.9.0",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "np": "^5.1.1",
    "prettier": "^1.18.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-test-renderer": "^16.10.2",
    "rollup": "^1.17.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "sort-package-json": "^1.22.1"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "np": {
    "yarn": false
  }
}
